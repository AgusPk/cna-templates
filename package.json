{
  "name": "playground.js",
  "version": "1.0.0",
  "description": "A basic template that consists of the essential elements that are required to start building a React application using Redux",
  "scripts": {
    "build": "webpack",
    "build:dev": "yarn build --mode development",
    "build:dev:watch": "yarn build:dev --watch",
    "build:dev:bundleanalyze": "yarn build:dev --env.addons=bundleanalyze",
    "build:dev:bundlebuddy": "yarn build:dev --env.addons=bundlebuddy",
    "serve:dev": "webpack-dev-server --mode development --hot",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server --mode development --hot --env.addons=dashboard",
    "build:prod": "yarn build -p",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:bundleanalyze": "yarn build:prod --env.addons=bundleanalyze",
    "build:prod:bundlebuddy": "yarn build:prod --env.addons=bundlebuddy",
    "serve:prod": "yarn build:prod && live-server ./dist",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "start": "yarn serve:dev",
    "test": "echo \"No tests available\"; exit 0",
    "webpack-defaults": "webpack-defaults"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulises-jeremias/playground.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "live-server": "^1.2.0",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1"
  },
  "dependencies": {
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^0.1.18",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "js-cookie": "^2.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-cookie": "^0.5.9",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.0",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.1"
  }
}
